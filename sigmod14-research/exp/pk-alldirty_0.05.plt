# Our Approach v.s. AllDirty 
reset

set terminal postscript eps enhanced color font "Times-Roman, 16"
set output "pk-alldirty-0.05.eps"

set size 1.5, 0.5
set multiplot
set ylabel "Query Result"     offset 1, 0 font "Times-Roman, 20"
set xlabel "Cleaned Sample Size" offset 0, 0.2 font "Times-Roman, 20"

#set xtics scale 0.5
#set xtics font "Times-Roman, 20"
#set ytics scale 0.5
#set ytics font "Times-Roman, 20"
#set key font "Times-Roman, 22"
#set key spacing 0.7
#set key samplen 1

#set size 1,0.9
set nomxtics
set noxtics


set grid xtics ytics
set grid mxtics mytics
set pointsize 1


#set logscale y 10
#unset logscale y


#set format y "%g%%"


set origin 0, 0
set size 0.5, 0.5
set xrange [ 0 : 10500 ]
set yrange [ 0 : 30]
set ytics 5   
set mytics 2
set key at graph 0.95, graph 0.6
set xtics 2000
set mxtics  2


